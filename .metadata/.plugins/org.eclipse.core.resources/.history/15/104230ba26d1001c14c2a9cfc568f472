package Recursion;

import java.util.*;
import java.lang.*;
import java.io.*;

public class HelloRcur
{
	public static void main (String[] args) throws java.lang.Exception
  {     

	
        int n = 8;
        
    
               
         	  System.out.println(fibonacci(n));
            	   
       
	}

	public static int fibonacci(int n)
	{
		if(n < 2)
			return n;
		return fibonacci(n-2)+fibonacci(n-1);
	}

}
