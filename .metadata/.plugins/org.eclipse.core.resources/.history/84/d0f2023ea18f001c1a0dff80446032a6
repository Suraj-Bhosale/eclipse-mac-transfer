package Oops;
import java.util.Scanner;
public class Patterns {

	public static void main(String[] args) {
		//Scanner scan = new Scanner(System.in);
		int row = 3;
		int col = 3;
		
		for(int i = 0; i <= row; i++) {
			for(int j = 0; j<= col; j++) {
				if(i == 0 || j == 0 || i == row-1 || j == col-1) {
				System.out.print("*");
				}
				else {
					System.out.print(" ");
				}
			}
			System.out.println();
		}
		

	}

}
